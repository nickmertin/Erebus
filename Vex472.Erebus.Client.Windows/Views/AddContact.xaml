<Page x:Class="Vex472.Erebus.Client.Windows.Views.AddContact"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Vex472.Erebus.Client.Windows.Views"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Margin="15">
        <TextBlock x:Name="text" HorizontalAlignment="Center" Margin="5"/>
        <TextBox x:Name="name" HorizontalAlignment="Stretch" Margin="5" TabIndex="0" KeyDown="key"/>
    </StackPanel>
</Page>